# rust.snippets

snippet fn "fn"
fn ${1:name}(${2})${3} {
    ${0}
}
endsnippet

snippet main "main"
fn main() {
    ${0}
}
endsnippet

snippet let "let"
let ${1}: ${2} = ${3};
endsnippet

snippet letmut "let mut"
let mut ${1}: ${2} = ${3};
endsnippet

snippet for "for"
for ${1:item} in ${2} {
    ${0}
}
endsnippet

snippet macro "macro_rules!"
macro_rules! ${1:name} {
    (${2:matcher}) => (
        ${3}
	)
}
endsnippet

snippet lambda "lambda"
${1:move}|${2}| { ${3} }
endsnippet
